#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules:/Users/sapana/Documents/vitesse1/node_modules:/Users/sapana/Documents/node_modules:/Users/sapana/node_modules:/Users/node_modules:/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/pinia@2.0.2_typescript@4.4.4+vue@3.2.21/node_modules/pinia/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/pinia@2.0.2_typescript@4.4.4+vue@3.2.21/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/typescript@4.4.4/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules:/Users/sapana/Documents/vitesse1/node_modules:/Users/sapana/Documents/node_modules:/Users/sapana/node_modules:/Users/node_modules:/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/pinia@2.0.2_typescript@4.4.4+vue@3.2.21/node_modules/pinia/node_modules:/Users/sapana/Documents/vitesse1/vitesse-app/node_modules/.pnpm/pinia@2.0.2_typescript@4.4.4+vue@3.2.21/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.4.4/node_modules/typescript/bin/tsc" "$@"
fi
